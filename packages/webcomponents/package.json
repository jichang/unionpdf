{
  "name": "@unionpdf/webcomponents",
  "version": "0.34.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "Web Components for unionpdf - framework-agnostic PDF viewer and editor",
  "source": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "start": "parcel --no-cache ./demo/index.html",
    "build": "parcel build",
    "postbuild": "cpy ./dist/types/* ./dist/cjs --flat && cpy ./dist/types/* ./dist/esm --flat",
    "clean": "rm -rf dist .parcel-cache",
    "purge": "rm -rf dist .parcel-cache node_modules",
    "test": "jest --passWithNoTests"
  },
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jichang/unionpdf.git"
  },
  "keywords": [
    "PDF",
    "Viewer",
    "Web Components",
    "Custom Elements",
    "Editor",
    "Pdfium"
  ],
  "author": "Ji Chang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jichang/unionpdf/issues"
  },
  "homepage": "https://github.com/jichang/unionpdf#readme",
  "devDependencies": {
    "@unionpdf/engines": "0.34.0",
    "@unionpdf/pdfium": "0.34.0",
    "@types/jest": "^29.5.14",
    "cpy-cli": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "2.13.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@unionpdf/models": "0.34.0"
  }
}
