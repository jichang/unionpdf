{
  "name": "@unionpdf/models",
  "version": "0.32.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "core models of unionpdf",
  "source": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .parcel-cache",
    "purge": "rm -rf dist .parcel-cache node_modules",
    "build": "parcel build",
    "postbuild": "cpy dist/types/* dist/cjs/ --flat && cpy dist/types/* dist/esm --flat",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jichang/unionpdf.git"
  },
  "keywords": [
    "PDF",
    "models"
  ],
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types"
  ],
  "author": "Ji Chang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jichang/unionpdf/issues"
  },
  "homepage": "https://github.com/jichang/unionpdf#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "cpy-cli": "^5.0.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "2.13.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "lmdb": "2.6.0"
  }
}
