{
  "name": "unionpdf",
  "private": false,
  "version": "0.33.1",
  "workspaces": [
    "packages/pdfium",
    "packages/models",
    "packages/engines",
    "packages/react"
  ],
  "devDependencies": {
    "@parcel/packager-ts": "2.13.3",
    "@parcel/transformer-typescript-types": "2.13.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^9.19.0",
    "events": "^3.3.0",
    "husky": "^9.1.7",
    "path-browserify": "^1.0.1",
    "prettier": "^3.4.2",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3",
    "vm-browserify": "^1.1.2"
  },
  "scripts": {
    "doc": "typedoc",
    "clean": "npm run clean --workspaces --if-present && rm -rf .parcel-cache",
    "purge": "npm run purge --workspaces --if-present && rm -rf .parcel-cache node_modules",
    "build": "npm run build --workspaces --if-present",
    "typecheck": "npm run typecheck --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "e2e": "npm run e2e --workspaces --if-present",
    "publish": "npm run build && npm publish --workspaces --if-present",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "prepare": "husky install"
  },
  "overrides": {
    "lmdb": "2.6.0"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
